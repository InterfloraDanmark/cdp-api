{
  "name": "interflora/cdp-api",
  "type": "library",
  "description": "Interflora cpd api module",
  "keywords": [
    "API"
  ],
  "homepage": "https://interflora.dk",
  "authors": [
    {
      "name": "Jesper Bisgaard",
      "email": "jesper@adapt.dk",
      "homepage": "https://interflora.dk",
      "role": "CTO"
    }
  ],
  "require": {
    "php": ">=7.2",
    "guzzlehttp/guzzle": "^6.2",
    "symfony/validator": ">=3.4",
    "symfony/cache": ">=3.4",
    "symfony/serializer": "3.4.*|4.3.*"
  },
  "autoload": {
    "psr-4" : {
      "Interflora\\CdpApi\\" : "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Interflora\\CdpApi\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.1",
    "mockery/mockery": "^1.1"
  }
}
